{
  "name": "truffle-ai",
  "version": "2.0.1",
  "description": "An SDK to interact with serverless AI agents using Truffle AI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "author": "Truffle AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/truffle-ai/truffle-ai-sdk.git"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/truffle-ai/truffle-ai-sdk/issues"
  },
  "homepage": "https://github.com/truffle-ai/truffle-ai-sdk#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "rimraf": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
